{
  "name": "covid19india-react-clone",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc --skipLibCheck true && vite build",
    "serve": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.+(js|ts|jsx|tsx|json|yml|yaml|md|html)\"",
    "prepare": "husky install",
    "sass-lint": "sass-lint 'src/**/*.scss' -v -f table --max-warnings 0",
    "sass-lint:fix": "sass-lint-auto-fix 'src/**/*.scss' -d -h -c .sass-lint-auto-fix.yml",
    "deploy": "gh-pages -d dist"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "yarn run lint:fix",
      "prettier --write \"**/*.+(js|jsx|ts|tsx)\""
    ],
    "*.+(json|yml|yaml|md)": [
      "prettier --write \"**/*.+(json|yml|yaml|md)\""
    ]
  },
  "dependencies": {
    "@primer/octicons-react": "^16.1.1",
    "classnames": "^2.3.1",
    "d3-fetch": "^3.0.1",
    "d3-geo": "^3.0.1",
    "d3-selection": "^3.0.0",
    "date-fns": "^2.27.0",
    "date-fns-tz": "^1.1.6",
    "i18next": "^21.4.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-http-backend": "^1.3.1",
    "immer": "^9.0.7",
    "react": "^17.0.0",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.0",
    "react-feather": "^2.0.9",
    "react-i18next": "^11.14.2",
    "react-router-dom": "^5.3.0",
    "react-spring": "^9.3.0",
    "react-use": "^17.3.1",
    "swr": "^1.0.1",
    "topojson-client": "^3.1.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@types/d3-fetch": "^3.0.1",
    "@types/d3-geo": "^3.0.2",
    "@types/d3-selection": "^3.0.2",
    "@types/node": "16.10.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.1",
    "@types/topojson-client": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "@welldone-software/why-did-you-render": "^6.2.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "14.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-wesbos": "2.0.1",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.2",
    "prettier": "2.4.1",
    "sass": "^1.43.2",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.2",
    "typescript": "4.4.3",
    "vite": "^2.5.4"
  }
}
